{
  "name": "mypocket",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "debug": "nodemon ./bin/www",
    "robot": "node --harmony-async-await ./tools/mybot",
    "test": "node ./tools/test",
    "servehealthcheck": "node ./tools/serve_health_check",
    "web-start": "NODE_ENV=production forever start --uid \"my-pocket\" -l `pwd`/logs/forever.log -o `pwd`/logs/out.log -e `pwd`/logs/err.log -a bin/www",
    "web-stop": "forever stop my-pocket",
    "robot-start": "docker-compose run -d wechaty tools/mybot.js",
    "robot-stop": "docker-compose down",
    "schedule-start": "forever start --uid \"my-pocket-schedules\" ./tools/schedules.js",
    "schedule-stop": "forever stop my-pocket-schedules"
  },
  "dependencies": {
    "async": "^2.4.0",
    "asyncawait": "^1.0.6",
    "bluebird": "^3.5.0",
    "body-parser": "~1.17.1",
    "chart.js": "^2.6.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "dotenv": "^4.0.0",
    "ejs": "^2.5.6",
    "express": "~4.15.2",
    "log4js": "^1.1.1",
    "mongoose": "^4.9.9",
    "morgan": "~1.8.1",
    "node-schedule": "^1.2.3",
    "nodemailer": "^4.0.1",
    "nodemailer-smtp-transport": "^2.7.4",
    "pug": "~2.0.0-beta11",
    "raven": "^2.0.2",
    "request-promise": "^4.2.1",
    "serve-favicon": "~2.4.2",
    "wechaty": "0.8.19"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "qrcode-terminal": "0.11.0",
    "request": "^2.81.0"
  }
}
